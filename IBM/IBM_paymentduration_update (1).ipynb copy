        "client_id = input(\"Enter client_id: \")\n",
        "commodity = input(\"Enter commodity: \")\n",
        "\n",
        "if client_id not in data['CLIENT_ID'].values:\n",
        "    print(\"no client found\")\n",
        "else:\n",
        "    # check if commodity is present in the dataframe\n",
        "    if commodity not in data['IBM_COMMODITY'].values:\n",
        "        print(\"no commodity found\")\n",
        "    else:\n",
        "        # filter the dataframe based on the user's input\n",
        "        result = data.loc[(data['CLIENT_ID'] == client_id) & (data['IBM_COMMODITY'] == commodity)]\n",
        "        \n",
        "        # group by supplier and get the index of the max money saved for each supplier\n",
        "        idx = result.groupby('SUPPLIER_NORMALIZED_NAME')['Money_saved'].idxmax()\n",
        "        \n",
        "        # use the index to get the rows with the most money saved for each supplier\n",
        "        result_sorted = result.loc[idx].sort_values(by='Money_saved', ascending=False).head(5)\n",
        "\n",
        "        # display the resulting dataframe\n",
        "        print(result_sorted[['SUPPLIER_NORMALIZED_NAME', 'avg_payment', 'Money_saved']])\n"
      ],
